\section{$\beta$-Functions}

Finally, the $\beta$-functions can be determined...
\begin{subequations} \label{App_II-Beta_functions}  
\begin{align}
	\dot{g} =& -\frac{g^2}{2\pi^2}\log K^2
	\label{App_II-Beta_functions:g} 
	\\[.75em]
	\dot{g}^{\{1\}}_{\text{\tiny PH}} =& -g^{\{1\}}_{\text{\tiny PH}} + \frac{g^2}{2(2\pi)^2}8 K^2
	\label{App_II-Beta_functions:g1} 
	\\[.75em]
	\dot{g}^{\{2)}_{\text{\tiny PH}} =& -2g^{\{2\}}_{\text{\tiny PH}} + \frac{g^2}{2(2\pi)^2}
	\Big( 
		19.7\log\frac{\mu}{K^2} 
		+  
		101
		-
		5.6K^4
	\Big)
	\label{App_II-Beta_functions:g2} 
	\\[.75em]
	\dot{g}^{\{3\}}_{\text{\tiny PH}} =& -3g^{\{3\}}_{\text{\tiny PH}} + \frac{g^2}{2(2\pi)^2}
	\Big( 
		-
		\frac{1.87}{\mu}
		+
		\Big[
			22.8K^2
			+
			\frac{86}{K^2}
		\Big]\log\frac{\mu}{K^2}
		+
		4.11K^6 
		+
		162K^2 
	\Big)
	\label{App_II-Beta_functions:g3}   
	\\[.75em]
	\dot{g}^{\{4\}}_{\text{\tiny PH}} =& -4g^{\{4\}}_{\text{\tiny PH}} + \frac{g^2}{2(2\pi)^2}
	\Big( 
		-
			\frac{0.13}{\mu^2}
			-
			2.06\frac{K^2}{\mu}
			+
			\Big[
				\frac{237}{K^4}
				+
				255
				+
				8.7 K^4
			\Big]\log\frac{\mu}{K^2}
	\nonumber \\
	& 
			+
			1462
			-
			71.5K^4
			-
			3.16K^8 
	\Big)
	\label{App_II-Beta_functions:g4}  
	\\[.75em]
	\dot{g}^{\{5\}}_{\text{\tiny PH}} =& -5g^{\{5\}}_{\text{\tiny PH}} + \frac{g^2}{2(2\pi)^2}
	\Big( 
			-
			\frac{0.014}{\mu^3}
			-
			\frac{0.138 K^2}{\mu^2}
			-
			\frac{0.326}{K^2\mu^2}
			-
			0.716\frac{K^4}{\mu}
			-
			\frac{17.13}{\mu}
	\nonumber \\
	& 
			-
			\frac{12.98}{K^4\mu}
			+\Big[
				-
				1.262K^6 
				+
				305K^2
				+
				\frac{1082}{K^2}
				+
				\frac{483}{K^6}
			\Big]\log\frac{\mu}{K^2} 
			+
			2.52K^{10}
	\nonumber \\
	& 
			+
			9.235K^6
			+
			1987K^2
	\Big)
	\label{App_II-Beta_functions:g5}
	\,.
\end{align}
\end{subequations}




\end{comment}











%--------------------------------
\newpage
\section{Computation of $\chi''_{\text{\tiny PH}}(q\hat{x},\Omega)$}


Here we present the computation of the imaginary part of the one-loop particle-hole susceptibility, $\chi''_{\text{\tiny PH}}(q\hat{x},\Omega)$, for the dispersion $\varepsilon_{\mathbf{k}}=k_x^2-k_y^2$ at a finite chemical potential and with a smooth momentum cutoff $K$. This susceptibility is obtained from the imaginary-time (Matsubara) susceptibility $\chi_{\text{\tiny PH}}(q\hat{x},\Omega)$ by analytically continuing $\chi_{\text{\tiny PH}}(q\hat{x},i\Omega\to \Omega+ i0^+)$~\cite{NegeleOrland1998}. The imaginary-time particle-hole susceptibility is given by:

\begin{align}
	\chi_{\text{\tiny PH}}(q\hat{x},i\Omega)
	&\,=\,
	\frac{-1}{(2\pi)^2} \int\text{d}\mathbf{k}\,
	\frac{\theta(\xi_{\mathbf{k}}-\mu)-\theta(\xi_{\mathbf{k+q}}-\mu)}{i\Omega+\xi_\mathbf{k}-\xi_\mathbf{k+q}}
	\,\,e^{-\frac{|\mathbf{k}|^2+|\mathbf{k+q}|}{K^2}}
	\,.
\end{align}
Following the same steps as in Section~[\ref{Appendix_II-GammaPH_NZmu}] (see Eq.~[\ref{App_II-Equation_4}]), the above integral is written as:
\begin{align}
	\chi_{\text{\tiny PH}}(\mathbf{q},i\Omega)
	\,=\,&
	\frac{-2}{2(2\pi)^2}
	\text{Re}
	\Bigg\{
	\int_{0}^{\infty}\text{d}X\,\int_{-1}^{1}\text{d}t\,\,
	\frac{X}{i\Omega + X}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{(X+q^2)^2-4q^2(\frac{t+1}{2}X+\mu)}}
	\Bigg\}	
	\nonumber \\[.5em]
	\,\,&
	\frac{-1}{(2\pi)^2}
	\int_{0}^{\infty}\text{d}X\,\int_{-1}^{1}\text{d}t\,\,
	\frac{X^2}{\Omega^2 + X^2}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{(X+q^2)^2-4q^2(\frac{t+1}{2}X+\mu)}}
	\label{App_II-ImTimeChi_PH}
	\,,
\end{align}
with the constraint $X^2 + 2tq^2X + q^4 - 4q^2\mu\geq0$. The constraint $X^2 + 2tq^2X + q^4 - 4q^2\mu\geq0$, for a generic $q$, implies $X\not\in(X_-,X_+)$ where $X_\pm=-tq^2\pm\sqrt{4q^2\mu-q^4(1-t^2)}$. As in Section~[\ref{Appendix_II-GammaPH_NZmu}], for $q\leq 2\sqrt{\mu}$ this constraint restricts the integration over $X$ to $[X_{\text{\tiny min}},\infty)$ where $X_{\text{\tiny min}}=-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)}$. 


Since we are interested in $\chi''_{\text{\tiny PH}}(\mathbf{q},\Omega)$ not only at small $q$ ($q\leq 2\sqrt{\mu}$) but also as $q\simeq \mathbf{O}(\mu)$ we need to consider the two cases $q\leq 2\sqrt{\mu}$ and $q > 2\sqrt{\mu}$ separately.



%===================================
\section*{\centering$q<2\sqrt{\mu}$}

For $q<2\sqrt{\mu}$, using $X\to\mathcal{Y} = X - X_{\text{\tiny min}}$, the above integral becomes:

\begin{align}
	\chi_{\text{\tiny PH}}(\mathbf{q},i\Omega)
	\,=\,&
	\frac{-1}{(2\pi)^2}e^{\frac{2\mu+q^2}{K^2}}
	\int_{0}^{\infty}\text{d}\mathcal{Y}\,\int_{-1}^{1}\text{d}t\,\,
	\frac{(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}{-(i\Omega)^2 + (\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}
	\nonumber \\[.5em]
	& \qquad \qquad \qquad
	\frac{e^{-\frac{t}{K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})-\frac{1}{q^2K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}}
	     {\sqrt{\mathcal{Y}(\mathcal{Y}+2\sqrt{4q^2\mu-q^4(1-t^2)})}}
	\,.
\end{align}
Analytically continuing $i\Omega\to\Omega + i0^+$, we find the retarded particle hole susceptibility:
\begin{align}
	\chi^{R}_{\text{\tiny PH}}(\mathbf{q},\Omega)
	\,=\,&
	\frac{-e^{\frac{2\mu+q^2}{K^2}}}{(2\pi)^2}
	\int_{0}^{\infty}\text{d}\mathcal{Y}\int_{-1}^{1}\text{d}t
	\frac{(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}{-(\Omega+i0^+)^2 + (\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}
	\nonumber
	\\[.5em]
	&
	\frac{e^{-\frac{t}{K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})-\frac{1}{q^2K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}}
	     {\sqrt{\mathcal{Y}(\mathcal{Y}+2\sqrt{4q^2\mu-q^4(1-t^2)})}}
	\,.
\end{align}
In the part of the integrand that contains $\Omega$, using the Dirac identity, we get:
\begin{align}
	\frac{1}{\underbrace{(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}_{\mathcal{X}>0}-(\Omega+i0^+)^2}
	\,=\,&
	\frac{1}{(\mathcal{X}-\Omega-i0^+)(\mathcal{X}+\Omega+i0^+)}
	\,.
\end{align} 
Without loss of generality, let us assume $\Omega>0$. We can write:
\begin{align}
	\frac{1}{\mathcal{X}-(\Omega+i0^+)^2}
	\,=\,&
	\frac{1}{(\mathcal{X}-\Omega-i0^+)(\mathcal{X}+\Omega)}
	\,.
\end{align}
Therefore, for $\Omega>0$, the imaginary part of the retarded real-time particle-hole susceptibility is given by:
\begin{align}
	\chi''_{\text{\tiny PH}}(\mathbf{q},\Omega)
	\,=\,
	\frac{-(-\pi)}{(2\pi)^2}e^{\frac{2\mu+q^2}{K^2}}
	&\int_{0}^{\infty}\text{d}\mathcal{Y}\,\int_{-1}^{1}\text{d}t\,\,
	\frac{(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}{ \Omega + \mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)}}
	\nonumber	
	\\[.5em]
	&  
	\frac{e^{-\frac{t}{K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})-\frac{1}{q^2K^2}(\mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)})^2}}
	     {\sqrt{\mathcal{Y}(\mathcal{Y}+2\sqrt{4q^2\mu-q^4(1-t^2)})}}
	\nonumber	
	\\[.5em]
	&
	\delta\Big( \mathcal{Y}-tq^2+\sqrt{4q^2\mu-q^4(1-t^2)} - \Omega \Big)
	\,.
\end{align}
Imposing the $\delta$-function, we finally obtain:
\begin{align}
	&\chi''_{\text{\tiny PH}}(\mathbf{q},\Omega)
	\,=\,
	-\frac{\Omega}{8\pi}\,	
	e^{\frac{2\mu+q^2}{K^2}-\frac{\Omega^2}{q^2K^2}}\,\,
	\int_{-1}^{1}\text{d}t\,\,
	\frac{e^{-\frac{t\Omega}{K^2}}}
	     {\sqrt{\Omega^2 + 2 t\Omega q^2 - 4q^2\mu + q^4 }}
	\,.
	\,.
\end{align}
with the constraint $\Omega + t q^2 - \sqrt{4q^2\mu-q^4(1-t^2)} \geq 0$. This constraint restricts the integration over $t$: 
\begin{equation}
	t\geq \frac{q^2}{2\Omega} ( \frac{4\mu}{q^2} - 1 - \frac{\Omega^2}{q^4} )
\end{equation}
Finally we find:
\begin{align}
	\chi''_{\text{\tiny PH}}(\mathbf{q},\Omega)
	\,=\,
	\color{blue}
	-\frac{\Omega}{8\pi}\,	
	e^{\frac{2\mu+q^2}{K^2}-\frac{\Omega^2}{q^2K^2}}\,\,
	\int_{\text{Max}\{ -1 , \frac{4\mu-q^2}{2\Omega^2} - \frac{\Omega}{2q^2} \} }^{1}\text{d}t\,\,
	\frac{e^{-\frac{t\Omega}{K^2}}}
	     {\sqrt{\Omega^2 + 2 t\Omega q^2 - 4q^2\mu + q^4 }}
	\,.
	\label{ql2smu}
\end{align}



%===================================
\section*{\centering $q\geq2\sqrt{\mu}$}


Next, we consider the $q\geq2\sqrt{\mu}$. Starting from Eq.~[\ref{App_II-ImTimeChi_PH}] after analytically continuing $i\Omega\to\Omega + i0^+$ and assuming that $\Omega>0$, we get:
\begin{align}
	\chi^{R}_{\text{\tiny PH}}(\mathbf{q},\Omega>0)
	\,=\,&
	\frac{-1}{(2\pi)^2}
	\int_{0}^{\infty}\text{d}X\,\int_{-1}^{1}\text{d}t\,\,
	\frac{X^2}{(X+\Omega)(X-\Omega-i0^+)}
	\nonumber	
	\\
	&
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{(X+q^2)^2-4q^2(\frac{t+1}{2}X+\mu)}}
	\label{App_II_RealChiPH_qg2mu_0}
	\,,
\end{align}
with the constraint $X^2+q^4 + 2t q^2X -4q^2\mu\geq0$. Imposing this constraint restricts the $X$ and $t$ integrals as following:
\begin{align}
	\chi^{R}_{\text{\tiny PH}}(q\geq2\sqrt{\mu},\Omega>0)
	\,=\,
	\frac{-1}{(2\pi)^2}
	\Bigg[
	&
		\underbrace{
			\int_{0}^{\infty}\text{d}X\,
			\int_{-\sqrt{1-\frac{4\mu}{q^2}}}^{1}\text{d}t\,\,
		}_{I_1}	
		+
		\underbrace{
			\int_{X_+}^{\infty}\text{d}X\,
			\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
		}_{I_2}	
	\nonumber	
	\\[.5em]
	&
		+
		\underbrace{
			\int_{0}^{X_-}\text{d}X\,
			\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
		}_{I_3}	
	\Bigg]
	(\cdots)
	\,,
\end{align}
where, $(\cdots)$ stands for the integrand, which is the integrand in Eq.[\ref{App_II_RealChiPH_qg2mu_0}]. We compute the above three integrals $I_1$, $I_2$ and $I_3$ separately. 

For $I_1$ we find:
\begin{align}
	I_1
	\,=\,&
	\frac{-1}{(2\pi)^2}
	\int_{0}^{\infty}\text{d}X\,
	\int_{-\sqrt{1-\frac{4\mu}{q^2}}}^{1}\text{d}t\,\,
	\frac{X^2}{(X+\Omega)(X-\Omega-i0^+)}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{X^2+q^4 + 2t q^2X -4q^2\mu}}
	\,.
\end{align}
Thus the imaginary part of $I_I$ is given by:
\begin{align}
	\text{Im}\{I_1\}
	\,=\,&
	\frac{-\pi}{(2\pi)^2}
	\int_{0}^{\infty}\text{d}X\,
	\int_{-\sqrt{1-\frac{4\mu}{q^2}}}^{1}\text{d}t\,\,
	\frac{X^2\delta(X-\Omega)}{(X+\Omega)}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{X^2+q^4 + 2t q^2X -4q^2\mu}}
	\nonumber \\
	=\,&
	\frac{-\pi}{(2\pi)^2}
	\int_{-\sqrt{1-\frac{4\mu}{q^2}}}^{1}\text{d}t\,\,
	\frac{\Omega}{2}
	\frac{e^{-\frac{1}{K^2}(t\Omega+\frac{\Omega^2}{q^2}-2\mu-q^2)}}{\sqrt{\Omega^2+q^4 + 2t q^2\Omega - 4q^2\mu}}
	\label{App_II_RealChiPH_qg2mu_Im-I_1}
	\,.
\end{align}

$I_2$ is given by a similar integral to the one for the case $q<2\sqrt{\mu}$,
\begin{align}
	I_2
	\,=\,&
	\frac{-1}{(2\pi)^2}
	\int_{X_+}^{\infty}\text{d}X\,
	\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
	\frac{X^2}{(X+\Omega)(X-\Omega-i0^+)}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{X^2+q^4 + 2t q^2X -4q^2\mu}}
	\,,
\end{align}
except that the upper limit of the $t$ integral is different. Thus for the imaginary part of $I_2$ we obtain:
\begin{align}
	\text{Im}\{I_2\}
	\,=\,&
	\frac{-\Omega}{8\pi}\,	
	e^{\frac{2\mu+q^2}{K^2}-\frac{\Omega^2}{q^2K^2}}\,\,
	\int_{ \text{Max}\{ -1 , \frac{4\mu-q^2}{2\Omega} - \frac{\Omega}{2q^2} \} }^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
	\frac{e^{-\frac{t\Omega}{K^2}}}
	     {\sqrt{\Omega^2 + 2 t\Omega q^2 - 4q^2\mu + q^4}}
	\label{App_II_RealChiPH_qg2mu_Im-I_2}
	\,,
\end{align}

For $I_3$ we have:
\begin{align}
	I_3
	=
	\frac{-1}{(2\pi)^2}
	\int_{0}^{X_-}\text{d}X
	\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t
	\frac{X^2}{(X+\Omega)(X-\Omega-i0^+)}
	\frac{e^{-\frac{1}{K^2}(tX+\frac{X^2}{q^2}-2\mu-q^2)}}{\sqrt{X^2+q^4 + 2t q^2X -4q^2\mu}}
	\,,
\end{align}
Using $X\to x=X_-X$, the above integral becomes:
\begin{align}
	I_3
	\,=\,&
	\frac{-1}{(2\pi)^2}
	\int_{0}^{1}\text{d}x\,
	\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
	\frac{X_-x^2}{(x+\frac{\Omega}{X_-})(x-\frac{\Omega}{X_-}-i0^+)}
	\frac{e^{-\frac{1}{K^2}(tX_x+\frac{X_-^2}{q^2}x^2-2\mu-q^2)}}{\sqrt{X_{-}^{2}x^2+q^4 + 2t q^2X_-x -4q^2\mu}}
	\,,
\end{align}
Thus the imaginary part of $I_3$ is given by:
\begin{align}
	\text{Im}\{I_3\}
	\,=\,&
	\frac{-\pi}{(2\pi)^2}
	\int_{0}^{1}\text{d}x\,
	\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,\,
	\frac{X_-x^2\delta{(x-\frac{\Omega}{X_-})}}{(x+\frac{\Omega}{X_-})}
	\frac{e^{-\frac{1}{K^2}(tX_x+\frac{X_-^2}{q^2}x^2-2\mu-q^2)}}{\sqrt{X_{-}^{2}x^2+q^4 + 2t q^2X_-x -4q^2\mu}}
	\nonumber \\[.5em]
	\,=\,&
	\frac{-\Omega}{8\pi}e^{-\frac{1}{K^2}(\frac{\Omega^2}{q^2}-2\mu-q^2)}
	\int_{-1}^{-\sqrt{1-\frac{4\mu}{q^2}}}\text{d}t\,
	\frac{\theta\big(1-\frac{\Omega}{X_-(t)}\big)\,e^{-\frac{t\Omega}{K^2}}}
	     {\sqrt{\Omega^2+q^4 + 2t q^2\Omega -4q^2\mu}}
	\label{App_II_RealChiPH_qg2mu_Im-I_3}
	\,,
\end{align}
The Heaviside function imposes the constraint $\frac{\Omega}{q^2}\leq-t-\sqrt{\frac{4\mu}{q^2}+t^2-1}$. 


Putting together the imaginary parts of $I_1$, $I_2$ and $I_3$ (Eqs.~[\ref{App_II_RealChiPH_qg2mu_Im-I_1},\ref{App_II_RealChiPH_qg2mu_Im-I_2},\ref{App_II_RealChiPH_qg2mu_Im-I_3}]), we finally obtain:
\begin{align}
	&
	\chi''_{\text{\tiny PH}}(q\geq2\sqrt{\mu},\Omega>0)
	\,=\,
	\color{blue}
	\frac{-\Omega\,e^{\frac{2\mu+q^2}{K^2}-\frac{\Omega^2}{q^2K^2}}}{2(2\pi)^2}
	\Bigg[
		\int_{-\sqrt{1-\frac{4\mu}{q^2}}}^{1}
	\,+\, 
		\int_{ \text{Max}\{ -1 , \frac{4\mu-q^2}{2\Omega} - \frac{\Omega}{2q^2} \} }^{-\sqrt{1-\frac{4\mu}{q^2}}}
	\,+\,
	\nonumber
	\\[.5em]
	&
	\color{blue}
		\theta(\frac{4\mu-q^2}{2\Omega}+\frac{\Omega}{2q^2})		
		\int_{\text{Max}\{ -1 , \frac{4\mu-q^2}{2\Omega}-\frac{\Omega}{2q^2} \}}^{-\sqrt{1-\frac{4\mu}{q^2}}}
	\Bigg]
	\frac{e^{-\frac{t\Omega}{K^2}}\,\text{d}t}{\sqrt{\Omega^2+q^4 + 2t q^2\Omega -4q^2\mu}}
	\,. 
	\label{qg2smu} 
\end{align}
The Heaviside function that accompanies the last integral imposes the constraint $( \frac{\Omega}{q^2} + \text{Max}\{ -1 , \frac{4\mu-q^2}{2\Omega}-\frac{\Omega}{2q^2} \}) <0$ as we are assuming $4\mu\leq q^2$. 






%===================================
\section*{\centering $\mu=0$}

Since we have computed $\chi''_{\text{\tiny PH}}(q,\Omega>0)$ for $\mu>0$ and a generic $q$, we can easily determine the $\mu=0$ susceptibility $\chi''_{\text{\tiny PH}}(q,\Omega>0)$ by simply taking the limit $\mu\to0$ (the zero chemical potential case). We find:

\begin{align}
	\chi''_{\text{\tiny PH}}(q,\Omega>0)
	\,=\,&
	\color{blue}
	\frac{-\Omega}{2(2\pi)^2}\,e^{\frac{q^2}{K^2}-\frac{\Omega^2}{q^2K^2}}\,
	\int_{-1}^{1}
	\frac{e^{-\frac{t\Omega}{K^2}}\,\text{d}t}{\sqrt{\Omega^2+q^4 + 2t q^2\Omega}}
	\,. 
\end{align}


%================================================================
